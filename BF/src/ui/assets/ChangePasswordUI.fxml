<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<Pane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="55.0" prefWidth="400.0" style="-fx-background-color: #333333;">
        <Label alignment="CENTER" text="修改密码" textFill="WHITE" AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
    </AnchorPane>
    <Label layoutX="30.0" layoutY="74.0" text="旧密码："/>
    <PasswordField fx:id="inputOldPassword" layoutX="120.0" layoutY="70.0" prefWidth="250.0" promptText="密码"/>
    <Label layoutX="25.0" layoutY="104.0" text="新密码："/>
    <PasswordField fx:id="inputNewPassword" layoutX="120.0" layoutY="100.0" prefWidth="250.0" promptText="密码"/>
    <Label layoutX="63.0" layoutY="134.0" text="确认新密码："/>
    <PasswordField fx:id="inputNewPassword2" layoutX="120.0" layoutY="130.0" onKeyPressed="#onInputKeyPressed"
                   prefWidth="250.0" promptText="密码"/>
    <Button defaultButton="true" layoutX="245.0" layoutY="163.0" mnemonicParsing="false" onAction="#onChangeAction"
            text="修改"/>
    <Button layoutX="315.0" layoutY="163.0" mnemonicParsing="false" onAction="#onCancelAction" text="取消"/>
</Pane>
